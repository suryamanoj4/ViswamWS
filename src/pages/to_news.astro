---
import Layout from '~/layouts/PageLayout.astro';
import Hero from '~/components/widgets/Hero.astro';
import Content from '~/components/widgets/Content.astro';
import Features from '~/components/widgets/Features.astro';
import Stats from '~/components/widgets/Stats.astro';

const metadata = {
    title: 'In the News',
    description: 'How Viswam AI has been working behind to be COE of AI for Global South',
};

const ainews = [
    {
        title: 'Crowdsourcing native LLMs: Viswam.ai’s innovative approach to AI for the Global South',
        description: 'In a groundbreaking initiative, the open source activists association Swecha and the International Institute of Information Technology - Hyderabad (IIIT-H) have joined forces to launch viswam.ai, a Centre of Excellence dedicated to developing AI solutions tailored to the unique needs of the Global South.',
        link: 'https://www.thehindubusinessline.com/info-tech/crowdsourcing-native-llms-viswamais-innovative-approach-to-ai-for-the-global-south/article69100974.ece',
        image: 'https://bl-i.thgim.com/public/todays-paper/tp-news/cz36s9/article69006570.ece/alternates/LANDSCAPE_1200/quickly_Inside_GT2DO8L42.3.jpg.jpg',
    },
    {
        title: 'Viswam.AI and Swecha Host Mega AI Hackathon at IIIT Hyderabad',
        description: 'AI Hackday, a mega AI hackathon, was conducted on 25th Jan 2025 as the first round towards kickstarting a series to build people-centric AI development.  With participation from 20+ colleges, 60+ faculty members, 100+ industry mentors nurtured 600+ teams with 2000+ students working on projects. This is organised by viswam.ai, a center setup at IIIT Hyderabad by IIITH and swecha.org.',
        link: 'https://smestreet.in/technology/viswamai-and-swecha-host-mega-ai-hackathon-at-iiit-hyderabad-8660823',
        image: 'https://img-cdn.thepublive.com/fit-in/1280x960/filters:format(webp)/smstreet/media/media_files/2025/01/08/diFfXx19tBRCqEEQmfXv.jpg',
    },
    {
        title: 'AI Ethics in Focus: EU Passes Stricter Regulations',
        description: 'The European Union has introduced new AI regulations to ensure transparency and accountanility in AI systems.',
        link: 'https://digital-strategy.ec.europa.eu/en/policies/regulatory-framework-ai',
        image: 'data:image/jpeg;base64,',
    },
];
---

<Layout metadata={metadata}>
    <Hero
        title="In the News"
    >
        <Fragment slot="subtitle">
            How Viswam AI has been working with various of its collaborators to speed up the process of making viswan a COE for the AI innovation wave in south for GLobal South
        </Fragment>
    </Hero>
<!-
    <div class="max-w-6xl mx-auto px-4 py-16 lg:px-8">
    <h2 class="text-3xl font-bold mb-8 text-center">AI News</h2>
        <div class="space-y-8">
            {ainews.map(news => (
                <div>
                    <h3 class="font-bold text-xl mb-2">{news.title}</h3>
                    <p class="text-muted">{news.description}<a href={news.link}><i>Read more...</i></a></p>
                </div>
                ))}
            </div>
    </div>
->
    <div class="max-w-6xl mx-auto px-4 py-16 lg:px-8">
    <h2 class="text-2xl font-bold mb-4 text-center">In the News</h2>
    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        {ainews.map(news => (
        <div class="bg-ciel rounded-2xl shadow-md p-4 hover:shadow-lg transition-shadow">
            <img src={news.image} alt="Image Not Found" class="w-full h-48 object-cover rounded-xl mb-4">
            <h3 class="text-xl font-semibold mb-2">{news.title}</h3>
            <p class="text-gray-600">{news.description}</p>
            <a href={news.link} class="text-blue-500 hover:underline mt-2 inline-block">Read more</a>
        </div>
        ))}
    </div>
    </div>

    <div class="text-center py-8 text-sm text-muted">
    <p>VISWAM.ai - Pronounced 'viswamayee'</p>
    <p>Venture for Innovation and Social Wellbeing through AI for the Marginalised</p>
    <p>A Joint Initiative of Swecha and IIIT Hyderabad</p>
    </div>
</Layout>
